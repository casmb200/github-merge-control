{
  "name": "__MSG_appName__",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/**",
        "https://*/**"
      ],
      "js": [
        "node_modules/jquery/dist/jquery.min.js",
        "app/content_scripts/merge_controller.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "app/options_page/option.html"
}
