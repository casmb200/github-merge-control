{
  "name": "GithubMergeControl",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "The merge button controll in Github",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/**",
        "https://*/**"
      ],
      "js": [
        "node_modules/jquery/dist/jquery.min.js",
        "app/merge_control.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "app/option.html"
}
